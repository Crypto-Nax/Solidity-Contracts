contract NftStaking is ERC721Holder, ReentrancyGuard, Ownable, Pausable {
    using Address for address;
    using SafeERC20 for IERC20;
    using SafeMath for uint256;
    using EnumerableSet for EnumerableSet.UintSet;

    struct UserInfo {
        uint256 rewardsDistributed;
        uint256 rewardDebt;
        uint256 amount;
        uint256 pendingRewards;
        EnumerableSet.UintSet nftIds;
        uint256 claimedAt;
    }

    uint256 stakers;
    uint256 lastUpdateTime;
    uint256 rewardRate;
    uint256 accPerShare;
    uint256 endTime;
    uint256 totalSupply;
    uint256 rewardCycle = 24 hours;

    mapping(address => UserInfo) userInfo;
    
    EnumerableSet.UintSet totalNfts;

    IERC721A StakingNftToken;
    IERC20 rewardToken;

    event NftBatchDeposit(address user, uint256[] NftIds);
    event NftDeposit(address user, uint256 NftId);
    event NftBatchWithdrawl(address user, uint256[] NftIds);
    event NftWithdrawl(address user, uint256 NftId);
    event Claimed(address user, uint256 amount);

    modifier updateRewards(){
        UserInfo storage user = userInfo[_msgSender()];
        if(block.number <= lastUpdateTime) return;
        if(totalNfts.length() == 0) return;
        if(totalSupply > 0) {
            uint256 multiplier = Math.min(block.timestamp, endTime).sub(lastUpdateTime);
            uint256 reward = multiplier.mul(rewardRate);
            accPerShare = accPerShare.add(reward);
            lastUpdateTime = Math.min(block.timestamp, endTime);
        }

        uint256 pending = user.amount.mul(accPerShare).div(1e12).sub(user.rewardDebt);
        user.pendingRewards = user.pendingRewards.add(pending);
        _;
        
        user.rewardDebt = user.amount.mul(accPerShare).div(1e12);
        if (user.claimedAt == 0) user.claimedAt = block.timestamp;
    }

    constructor(IERC20 Token, IERC721A Nft) {
        StakingNftToken = Nft;
        rewardToken = Token;
        endTime = block.timestamp + 1826.25 days; // 5 years
    }

    receive() external payable {}
    // =================== Owner Only ===============

    function pause() external onlyOwner {
        _pause();
    }

    function unpause() external onlyOwner {
        _unpause();
    }
    
    function ownerDeposit(uint256 amount) external onlyOwner {
        rewardToken.safeTransferFrom(_msgSender(), address(this), amount);
        totalSupply += amount;
    }

    function setRewardCycle(uint256 _cycleMinutes) external onlyOwner {
        rewardCycle = _cycleMinutes * 1 minutes;
    }

    function restartPeriod(uint256 _minutes) external onlyOwner {
        require (block.timestamp > endTime, "!expired");
        endTime = block.timestamp.add(_minutes * 1 minutes);
    }

    function setEndTime(uint256 time) external onlyOwner {
        require (block.timestamp < time, "Invalid time");
        endTime = time;
    }

    function setRewardRate(uint256 _rewardRate) internal {
        require (endTime > block.timestamp, "expired");
        require (_rewardRate > 0, "Rewards per second should be greater than 0!");

        // Update pool infos with old reward rate before setting new one first
        if (totalSupply > 0) {
            uint256 multiplier = block.timestamp.sub(lastUpdateTime);
            uint256 reward = multiplier.mul(rewardRate);
            accPerShare = accPerShare.add(reward.mul(1e12).div(totalSupply));    
        }
        lastUpdateTime = block.timestamp;
        rewardRate = _rewardRate;
    }

    // =================== Public ===============

    function claim() external nonReentrant updateRewards {
        require(!Address.isContract(_msgSender()), "Message sender cannot be a contract");
        UserInfo storage user = userInfo[_msgSender()];
        require(block.timestamp.sub(user.claimedAt) >= rewardCycle);
        if (user.amount > 0 && user.pendingRewards > 0) {
            uint pending = user.pendingRewards;
            rewardToken.safeTransfer(_msgSender(), pending);
            user.rewardsDistributed.add(pending);
            totalSupply.sub(pending);
            user.pendingRewards = 0;
            emit Claimed(_msgSender(), pending);
        }

    }

    function claimable() external view returns (uint256) {
        UserInfo storage user = userInfo[_msgSender()];
        if (user.pendingRewards == 0) return (0);
        uint256 curAccPerShare = accPerShare;
        if (totalSupply > 0) {
            uint256 multiplier = Math.min(block.timestamp, endTime).sub(lastUpdateTime);
            uint256 reward = multiplier.mul(rewardRate);
            curAccPerShare = accPerShare.add(reward.mul(1e12).div(totalSupply));
        }
        return user.pendingRewards.mul(curAccPerShare).div(1e12).sub(user.rewardDebt);
    }

    function depositNft(uint256 NftId) external nonReentrant whenNotPaused updateRewards {
        require(!Address.isContract(_msgSender()), "Message sender cannot be a contract");
        require(StakingNftToken.ownerOf(NftId) == _msgSender(), "You must be the owner of this nft");
        UserInfo storage NFTs = userInfo[_msgSender()];
        if (NFTs.amount > 0 && NFTs.pendingRewards > 0) {
            uint pending = NFTs.pendingRewards;
            rewardToken.safeTransfer(_msgSender(), pending);
            NFTs.rewardsDistributed.add(pending);
            NFTs.pendingRewards = 0;
            totalSupply.sub(pending);
            emit Claimed(_msgSender(), pending);
        }
        StakingNftToken.safeTransferFrom(_msgSender(), address(this), NftId);
        if(NFTs.nftIds.length() == 0) stakers++;
        NFTs.amount.add(1);
        NFTs.nftIds.add(NftId);
        totalNfts.add(NftId);
        emit NftDeposit( _msgSender(), NftId);

    }

    function batchDepositNft(uint256[] memory NftId) external nonReentrant whenNotPaused updateRewards {
        require(!Address.isContract(_msgSender()), "Message sender cannot be a contract");
        UserInfo storage NFTs = userInfo[_msgSender()];
        if (NFTs.amount > 0 && NFTs.pendingRewards > 0) {
            uint pending = NFTs.pendingRewards;
            rewardToken.safeTransfer(_msgSender(), pending);
            NFTs.rewardsDistributed.add(pending);
            NFTs.pendingRewards = 0;
            totalSupply.sub(pending);
            emit Claimed(_msgSender(), pending);
        }
        uint depositAmount;
        for(uint256 i; i < NftId.length; i++){
            if(StakingNftToken.ownerOf(NftId[i]) == _msgSender()){
                StakingNftToken.safeTransferFrom(_msgSender(), address(this), NftId[i]);
                if(NFTs.nftIds.length() == 0) stakers++;
                NFTs.nftIds.add(NftId[i]);
                totalNfts.add(NftId[i]);
                depositAmount++;
            }
        }
        NFTs.amount.add(depositAmount);
        emit NftBatchDeposit(_msgSender(), NftId);
    }

    function withdrawNft(uint256 NftId) external updateRewards {
        require(!Address.isContract(_msgSender()), "Message sender cannot be a contract");
        UserInfo storage NFTs = userInfo[_msgSender()];
        require(NFTs.nftIds.contains(NftId), "You do not own this nft");
        if (NFTs.amount > 0 && NFTs.pendingRewards > 0) {
            uint pending = NFTs.pendingRewards;
            rewardToken.safeTransfer(_msgSender(), pending);
            NFTs.rewardsDistributed.add(pending);
            NFTs.pendingRewards = 0;
            totalSupply.sub(pending);
            emit Claimed(_msgSender(), pending);
        }
        StakingNftToken.safeTransferFrom(address(this), _msgSender(), NftId);
        NFTs.amount.sub(1);
        NFTs.nftIds.remove(NftId);
        totalNfts.remove(NftId);
        if(NFTs.nftIds.length() == 0) stakers--;
        emit NftWithdrawl(_msgSender(), NftId);
    }

    function batchWithdrawNft(uint256[] memory NftId) external updateRewards {
        require(!Address.isContract(_msgSender()), "Message sender cannot be a contract");
        UserInfo storage NFTs = userInfo[_msgSender()];
        if (NFTs.amount > 0 && NFTs.pendingRewards > 0) {
            uint pending = NFTs.pendingRewards;
            rewardToken.safeTransfer(_msgSender(), pending);
            NFTs.rewardsDistributed.add(pending);
            NFTs.pendingRewards = 0;
            totalSupply.sub(pending);
            emit Claimed(_msgSender(), pending);
        }
        uint withdrawAmount;
        for(uint256 i; i < NftId.length; i++){
            if(NFTs.nftIds.contains(NftId[i])){
                StakingNftToken.safeTransferFrom(address(this), _msgSender(), NftId[i]);
                NFTs.nftIds.remove(NftId[i]);
                totalNfts.remove(NftId[i]);
                withdrawAmount++;
            }
        }
        if(withdrawAmount > 0) NFTs.amount.sub(withdrawAmount);
        if(NFTs.nftIds.length() == 0) stakers--;
        emit NftBatchWithdrawl(_msgSender(), NftId);
    }

    function getStakingDetails() external view returns(address nftAddress, address tokenAddress, uint256 _lastUpdateTime, uint256 _rewardRate, uint256 _accPerShare, uint256 _endTime, uint256 _totalSupply, uint256 _rewardCycle, uint256 _stakers) {
        nftAddress = address(StakingNftToken);
        tokenAddress = address(rewardToken);
        _lastUpdateTime = lastUpdateTime;
        _rewardRate = rewardRate;
        _accPerShare = accPerShare;
        _endTime = endTime;
        _totalSupply = totalSupply;
        _rewardCycle = rewardCycle;
        _stakers = stakers;
    }

    function getUserDetails() external view returns(uint256 _claimedAt, uint256 _rewardsDistributed){
        UserInfo storage user = userInfo[_msgSender()];
        _rewardsDistributed = user.rewardsDistributed;
        _claimedAt = user.claimedAt;
    }

    function usersNfts() external view returns (uint256[] memory, uint256 nftAmount) {
        return (userInfo[_msgSender()].nftIds.values(), userInfo[_msgSender()].nftIds.length());
    }

    function allNfts() external view returns (uint256[] memory nftIds, uint256 nftAmount) {
        nftIds = totalNfts.values();
        nftAmount = totalNfts.length();
    }
}
